
public class Ex7_3 {

	public static void main(String[] args) {
		Child c = new Child();
		c.method();
	}

}
class Parent2 {int x = 10;} //super.x와 this.x 둘다 가능

class Child extends Parent{
	void method() {
		System.out.println("x=" + x);
		System.out.println("this.x=" + this.x);
		System.out.println("super.x=" + super.x);
	}
}